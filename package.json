{
  "name": "tdwg-theme",
  "description": "A Pelican theme for the TDWG website",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "watch:css": "onchange 'src/scss/*.scss' -- npm run scss"
    "scss": "node-sass src/scss --output-style nested --source-map true --output static/css",
    "copyjs:custom": "rm -r static/js/ && cp -r src/js/ static/js/",
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.11.1"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "onchange": "^3.2.1"
  }
}
